{
  "name": "jupyterlab_wipp",
  "version": "0.2.0",
  "description": "WIPP integration with JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/labshare/jupyterlab-extensions/jupyterlab_wipp",
  "bugs": {
    "url": "https://github.com/labshare/jupyterlab-extensions/issues"
  },
  "license": "MIT",
  "author": "Konstantin Taletskiy",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/labshare/jupyterlab-extensions.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.1.3",
    "@jupyterlab/mainmenu": "^1.1.3",
    "@jupyterlab/notebook": "^1.1.3",
    "@jupyterlab/filebrowser": "^1.0.4",
    "@jupyterlab/console": "^1.1.3"    
  },
  "devDependencies": {
    "rimraf": "^2.7.1",
    "typescript": "~3.5.2"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
